    <validationRules>
        <fullName>WT_Goal_Metrics_Access_New</fullName>
        <active>true</active>
        <description>This rule is  to throw an error message to the user who tries to create a Goal metrics record other than WT Business Admin</description>
        <errorConditionFormula>AND(($Profile.Name !=&quot;WT Business Administrator&quot;),
     ($Profile.Name !=&quot;WT Technical Administrator&quot;),
     ($Profile.Name !=&quot;System Administrator&quot;),ISNEW())</errorConditionFormula>
        <errorMessage>The user does not have access to enter the goal metric.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WT_Goal_Metrics_Access_Update</fullName>
        <active>true</active>
        <description>This Rule is to throw an error message to the user other than WT Business Admin tries to update the Goal metrics record.</description>
        <errorConditionFormula>AND(($Profile.Name !=&quot;WT Business Administrator&quot;),
       ($Profile.Name !=&quot;WT Technical Administrator&quot;),
       ($Profile.Name !=&quot;System Administrator&quot;),
OR( ISCHANGED( WT_Actual_Result_Criteria__c),
   ISCHANGED( WT_Metric_Type__c),
   ISCHANGED( WT_Actual_Result_Criteria_Logic__c),
   ISCHANGED( WT_Actual_Result_Date_Field_API_Name__c),
   ISCHANGED( WT_InProgress_Result_Criteria__c),
   ISCHANGED( WT_In_Progress_Criteria_Logic__c),
   ISCHANGED( WT_InProgress_Result_Date_Field_API_Name__c),
   ISCHANGED( Name),
   ISCHANGED( WT_CRM_Field_API_Name__c),
   ISCHANGED( WT_CRM_Object_API_Name__c)))</errorConditionFormula>
        <errorDisplayField>WT_Actual_Result_Criteria__c</errorDisplayField>
        <errorMessage>The user does not have access to update the goal metric.</errorMessage>
    </validationRules>
