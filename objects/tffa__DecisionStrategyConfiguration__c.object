    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <deprecated>false</deprecated>
    <description>Configuration for performing Champion/Challenger testing on decision tables (or matrices) for a specific use case (purpose).  Champion/Challenger testing is the process of comparing two or more strategies or processes to determine which one performs best. e.g. testing efficacy of different Questionnaires used for Financial need capture. A configuration can have 1 Champion and many Challenger strategies. Depending on the weights of the strategies, a suitable one is
        chosen at runtime.</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>false</enableReports>
    <enableSearch>false</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>tffa__Active__c</fullName>
        <deprecated>false</deprecated>
        <description>Reports whether the configuration is active for consumption. Derived from disabled and effective period (StartDate &amp; EndDate) of the configuration.</description>
        <externalId>false</externalId>
        <formula>tffa__Disabled__c = False &amp;&amp; tffa__StartDate__c &lt;= TODAY() &amp;&amp; tffa__EndDate__c &gt;= TODAY()</formula>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>tffa__Disabled__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Captures explicit disabling of Champion/Challenger testing independent of any configured effective range.</description>
        <externalId>false</externalId>
        <label>Disabled</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>tffa__EndDate__c</fullName>
        <defaultValue>DATE(2049,1,1)</defaultValue>
        <deprecated>false</deprecated>
        <description>The date on which the Champion/Challenger testing expires. Captures the upper bound of the effective range.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>End Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>tffa__EntityKey__c</fullName>
        <deprecated>false</deprecated>
        <description>Combination of entityType and entityPurpose. e.g. QuestionnaireMatrix_FINANCIAL_NEED, ApplicationDecisionMatrix_KYC</description>
        <externalId>false</externalId>
        <formula>TEXT(tffa__EntityType__c) &amp; &quot;_&quot; &amp; TEXT(tffa__EntityPurpose__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Entity Key</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
