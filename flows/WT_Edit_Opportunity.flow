IF(INCLUDES({!varOpportunityToEdit.WT_Involves_any_of_the_product_partner__c},&quot;Syndication&quot;),&quot;Syndication&quot;&amp;BR(),null)&amp;
IF(INCLUDES({!varOpportunityToEdit.WT_Involves_any_of_the_product_partner__c},&quot;Wealth&quot;),&quot;Wealth&quot;,null)</expression>
    </formulas>
    <formulas>
        <name>varProbabilityFormula</name>
        <dataType>Number</dataType>
        <expression>IF(TEXT({!varOpportunityToEdit.StageName})=={!StageName},{!varProbabiltyValue},
   IF({!StageName}==&apos;Stage 1 - Opportunity Identified&apos;,10,
      IF({!StageName}==&apos;Stage 2 - Company Information Obtained/Needs Assessed&apos;,35,
           IF({!StageName}==&apos;Stage 3 - Deal Structured/Presented&apos;,50,
                 IF({!StageName}==&apos;Stage 4 - Deal Accepted/Commitment&apos;,75,
                     IF({!StageName}==&apos;Stage 5 - Deal Approved&apos;,90,
                         IF({!StageName}==&apos;Stage 6 - Deal Booked/Closed&apos;,100,
                              IF({!StageName}==&apos;Stage 7 - Deal Lost or Declined&apos;,0,{!varProbabiltyValue}))))))))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>varProFormulaCheck</name>
        <dataType>Boolean</dataType>
        <expression>IF(ISNULL({!varProductPartners}), True, IF({!varProductPartners}==&apos;&apos;, True, False))</expression>
    </formulas>
    <formulas>
        <name>varSBAComponentFormula</name>
        <dataType>String</dataType>
        <expression>IF({!SBA_Component}==&apos;Yes&apos;,&apos;Yes&apos;,&apos;No&apos;)</expression>
    </formulas>
    <formulas>
        <name>varSwapComponentFormula</name>
        <dataType>String</dataType>
        <expression>IF({!Swap_Component}==&apos;Yes&apos;,&apos;Yes&apos;,&apos;No&apos;)</expression>
    </formulas>
    <formulas>
        <name>varSyndicationComponentFormula</name>
        <dataType>String</dataType>
        <expression>IF({!Syndication_Component}==&apos;Yes&apos;,&apos;Yes&apos;,&apos;No&apos;)</expression>
    </formulas>
    <formulas>
        <name>varUnavailableSelectedProductPartner</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE({!varProductPartners}, &quot;ABL&quot;, &quot;&quot; ),&quot;SBA&quot;,&quot;&quot;),&quot;Swap&quot;,&quot;&quot;),&quot;Franchise&quot;,&quot;&quot;),&quot;Syndication&quot;,&quot;&quot;),&quot;Wealth&quot;,&quot;&quot;),&quot;;&quot;,&quot;&quot;),&quot; &quot;,&quot;&quot;)</expression>
